.projects
  - data.projects.each_with_index do |project,index|
    .item{class: "item-#{index + 1}"}
      %h3= project[:name]
      %p= project[:description]

      %b Technologies used: 
      %p #{project[:"Technologies-used"]}

      %p
        %a{:href => "#{project[:"Link-to-Github"]}"} View Github Repo

      %p
        %a{:href => "#{project[:"Link-to-Livesite"]}"} View Live Site


